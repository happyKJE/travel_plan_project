model User {
  id          Int           @id @default(autoincrement())
  email       String        @unique
  password    String
  phoneNumber String
  name        String?
  created_at  DateTime      @default(now())
  travelPlans TravelPlan[]
}

model TravelPlan {
  id          Int      @id @default(autoincrement())
  user_id     Int
  title       String
  date        String?
  description String?
  created_at  DateTime @default(now())

  user        User     @relation(fields: [user_id], references: [id], onDelete: Cascade)

  @@index([user_id])
}
