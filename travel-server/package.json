{
  "name": "travel-server",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "npm install && npm run generate && npx tsc",
    "start": "node dist/server.js",
    "generate": "prisma generate",
    "queries": "ts-node ./src/queries.ts",
    "caching": "ts-node ./src/caching.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "6.5.0",
    "@prisma/extension-accelerate": "1.2.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.12",
    "@types/node": "22.13.10",
    "prisma": "6.5.0",
    "ts-node": "10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.8.2"
  }
}
